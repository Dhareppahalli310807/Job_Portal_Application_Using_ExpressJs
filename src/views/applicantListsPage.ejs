<div class="container">
        <table class="table table-striped highlight responsive-table striped">
          <thead>
            <tr>
              <th scope="col">APPLICANTID</th>
              <th scope="col">Name</th>
              <th scope="col">Email</th>
              <th scope="col">Contact</th>
              <th scope="col">ResumePath</th>
            </tr>
          </thead>
          <tbody>
            <% if(allApplicants) {%>
            <%allApplicants.forEach((applicant)=> {%>
            <tr>
              <th scope="row"><%=applicant.applicantid%></th>
              <td><%=applicant.name%></td>
              <td><%=applicant.email%></td>
              <td><%=applicant.contact%></td>
              <td><%=applicant.resumePath</td>
              <td>
                <a
                  class="btn btn-sm btn-primary"
                  href="http://localhost:9090/resumes/<%=applicant.resumePath%>"
                  target="_blank"
                  >view</a
                >
              </td>
            </tr>
            <%})%>
            <%}%>
            
          </tbody>
        </table>
      </div>